#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/app-tools/bin/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/app-tools/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/app-tools/bin/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/app-tools/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules/@modern-js/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/@modern-js+app-tools@2.60.0_@rspack+core@1.0.5_@swc+helpers@0.5.13__@types+webpack@4.41.39_re_aohz2suaene4ixtuj6rpybepqy/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@modern-js/app-tools/bin/modern.js" "$@"
else
  exec node  "$basedir/../@modern-js/app-tools/bin/modern.js" "$@"
fi
