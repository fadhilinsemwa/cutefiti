#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules/husky/lib/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules/husky/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules/husky/lib/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules/husky/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/husky@8.0.3/node_modules:/home/fadhili/Documents/apps/ex/exf/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../husky@8.0.3/node_modules/husky/lib/bin.js" "$@"
else
  exec node  "$basedir/../../../../../../husky@8.0.3/node_modules/husky/lib/bin.js" "$@"
fi
