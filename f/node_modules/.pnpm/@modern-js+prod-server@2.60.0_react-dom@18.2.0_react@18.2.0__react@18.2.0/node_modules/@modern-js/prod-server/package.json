    "@modern-js/server-core": "2.60.0"
  },
  "devDependencies": {
    "@types/jest": "^29",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "~16.11.7",
    "@types/source-map-support": "0.5.10",
    "jest": "^29",
    "lint-staged": "~13.1.0",
    "tsx": "^3.12.7",
    "typescript": "~5.0.4",
    "@modern-js/types": "2.60.0",
    "@scripts/build": "2.60.0",
    "@scripts/jest-config": "2.60.0"
  },
  "sideEffects": [],
