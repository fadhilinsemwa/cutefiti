  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.60.0",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
